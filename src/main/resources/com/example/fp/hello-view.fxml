<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="673.0" prefWidth="541.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="com.example.fp.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane prefHeight="510.0" prefWidth="502.0">
         <children>
            <TableView fx:id="todotable" prefHeight="507.0" prefWidth="510.0">
              <columns>
                <TableColumn fx:id="todoName" prefWidth="448.0" text="todo" />
                <TableColumn fx:id="todoCheck" minWidth="0.0" prefWidth="61.0" text="check" />
              </columns>
            </TableView>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="54.0" prefWidth="501.0">
         <children>
            <Button fx:id="add" layoutX="413.0" layoutY="28.0" mnemonicParsing="false" onAction="#AddNewTask" prefHeight="58.0" prefWidth="62.0" text="+" />
            <TextArea fx:id="Text_WhatToDo" layoutX="6.0" prefHeight="113.0" prefWidth="388.0" wrapText="true" />
         </children>
      </AnchorPane>
   </children>
</VBox>
